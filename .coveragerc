[run]
source = .
omit =
    */migrations/*
    */venv/*
    */env/*
    conftest.py
    create_sample_data.py
    manage.py
    */test_*.py
    */tests.py
    pytest.ini
    requirements.txt

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
